{
  "expo": {
    "name": "SafetyApp Kenya",
    "slug": "safety-app-kenya",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to access your location."
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#EF4444"
        }
      ]
    ],
    "ios": {
      "supportsTabletMode": false,
      "bundleIdentifier": "com.safetyapp.kenya",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Your location is used to report emergencies to responders.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Your location is used for emergency response and location sharing.",
        "NSLocationAlwaysUsageDescription": "Your location is used for emergency response.",
        "NSUserNotificationUsageDescription": "Notifications are used for emergency alerts and updates."
      }
    },
    "android": {
      "package": "com.safetyapp.kenya",
      "versionCode": 1,
      "minSdkVersion": 24,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "POST_NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {},
    "updates": {
      "enabled": false
    },
    "extra": {
      "eas": {
        "projectId": "4821964b-d6ed-4808-afc8-59c835e52d9a"
      }
    }
  }
}
